namespace com.tlongdev.bktf.flatbuffers.itemschema;

table Origin {
  id:int;
  name:string;
}

table Item {
  defindex:int;
  name:string;
  description:string;
  type:string;
  proper:bool;
  image:string;
  imageLarge:string;
}

table Particle {
  id:int;
  name:string;
}

table DecoratedWeapon {
  defindex:int;
  grade:int;
}

table ItemSchema {
  origins:[Origin];
  items:[Item];
  particle:[Particle];
  decoratedWeapon:[DecoratedWeapon];
}

root_type ItemSchema;